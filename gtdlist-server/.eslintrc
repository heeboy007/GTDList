{
    "parser": "@typescript-eslint/parser",
    "plugins": [
    "@typescript-eslint",
    "import"
    ],
    "rules": {
    "import/extensions": ["error", "always", {
        "js": "never",
        "ts": "never"
    }]
    },
    "settings": {
    "import/resolver": {
        "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
        }
    }
    }
}
