{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
    "@typescript-eslint",
    "import"
    ],
    "rules": {
        "@typescript-eslint/no-unused-vars": "off",
        "import/extensions": ["error", "always", {
            "js": "never",
            "ts": "never"
        }]
    },
    "settings": {
    "import/resolver": {
        "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
        }
    }
    }
}
